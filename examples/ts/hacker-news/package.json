{
  "name": "hacker-news-ts",
  "version": "0.30.5",
  "private": true,
  "description": "Hacker News application clone with CQRS and EventSourcing",
  "scripts": {
    "build": "tsc && babel-node --extensions=.ts,.tsx ./run.js build",
    "cloud": "tsc && babel-node --extensions=.ts,.tsx ./run.js cloud",
    "cloud:replica": "tsc && babel-node --extensions=.ts,.tsx run.ts cloud:replica",
    "dev": "tsc && babel-node --extensions=.ts,.tsx run.ts dev",
    "dev:replica": "tsc && babel-node --extensions=.ts,.tsx run.ts dev:replica",
    "export-event-store": "tsc && babel-node --extensions=.ts,.tsx run.ts export-event-store",
    "import": "tsc && babel-node --extensions=.ts,.tsx run.ts import",
    "import-event-store": "tsc && babel-node --extensions=.ts,.tsx run.ts import-event-store",
    "reset": "tsc && babel-node --extensions=.ts,.tsx run.ts reset",
    "start": "tsc && babel-node --extensions=.ts,.tsx run.ts start",
    "test:e2e": "tsc && babel-node --extensions=.ts,.tsx run.ts test:e2e",
    "test:e2e-cloud": "tsc && babel-node --extensions=.ts,.tsx run.ts test:e2e-cloud",
    "module-admin": "module-admin",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "@babel/node": "7.14.2",
    "@elastic/elasticsearch": "7.11.0",
    "@resolve-js/debug-levels": "0.30.5",
    "@resolve-js/eventstore-lite": "0.30.5",
    "@resolve-js/eventstore-postgresql-serverless": "0.30.5",
    "@resolve-js/module-admin": "0.30.5",
    "@resolve-js/module-auth": "0.30.5",
    "@resolve-js/module-comments": "0.30.5",
    "@resolve-js/module-replication": "0.30.5",
    "@resolve-js/readmodel-lite": "0.30.5",
    "@resolve-js/readmodel-postgresql-serverless": "0.30.5",
    "@resolve-js/redux": "0.30.5",
    "@resolve-js/react-hooks": "0.30.5",
    "@resolve-js/replicator-via-api-handler": "0.30.5",
    "@resolve-js/scripts": "0.30.5",
    "history": "4.10.1",
    "isomorphic-fetch": "3.0.0",
    "js-cookie": "2.2.1",
    "jsonwebtoken": "8.5.1",
    "passport-local": "1.0.0",
    "progress": "2.0.3",
    "prop-types": "15.7.2",
    "query-string": "6.14.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-redux": "7.2.4",
    "react-router": "5.2.0",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.2.0",
    "redux": "4.1.0",
    "redux-devtools-extension": "2.13.9",
    "redux-saga": "1.1.3",
    "sanitizer": "0.1.3",
    "seamless-immutable": "7.1.4",
    "styled-components": "4.4.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@babel/core": "7.14.3",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "7.14.2",
    "@babel/plugin-transform-runtime": "7.14.3",
    "@babel/preset-env": "7.14.4",
    "@babel/preset-react": "7.13.13",
    "aws-sdk": "2.860.0",
    "aws-xray-sdk-core": "3.2.0",
    "babel-jest": "27.0.2",
    "jest": "27.0.3",
    "sinon": "9.2.4",
    "socket.io": "3",
    "testcafe": "1.14.2"
  },
  "resolveJs": {
    "isAppTemplate": true
  }
}