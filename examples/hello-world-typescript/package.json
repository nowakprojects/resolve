{
  "name": "hello-world-typescript",
  "version": "0.30.4",
  "private": true,
  "description": "used as a template for new reSolve applications",
  "scripts": {
    "build": "tsc && babel-node --extensions=.ts,.tsx run.ts build",
    "dev": "tsc && babel-node --extensions=.ts,.tsx run.ts dev",
    "start": "tsc && babel-node --extensions=.ts,.tsx run.ts start",
    "test": "jest --testMatch=**/test/unit/*.test.js --verbose --passWithNoTests",
    "test:e2e": "tsc && babel-node --extensions=.ts,.tsx run.ts test:e2e",
    "type-check": "tsc"
  },
  "resolutions": {
    "@types/react": "17.0.8"
  },
  "dependencies": {
    "@babel/node": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@resolve-js/eventstore-lite": "0.30.4",
    "@resolve-js/eventstore-postgresql-serverless": "0.30.4",
    "@resolve-js/react-hooks": "0.30.4",
    "@resolve-js/readmodel-base": "0.30.4",
    "@resolve-js/readmodel-lite": "0.30.4",
    "@resolve-js/readmodel-postgresql-serverless": "0.30.4",
    "@resolve-js/scripts": "0.30.4",
    "babel-loader": "8.2.2",
    "react": "17.0.2",
    "react-bootstrap": "1.6.0",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "7.12.13",
    "@babel/plugin-transform-runtime": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@types/node": "12.7.4",
    "@types/react": "17.0.8",
    "@types/react-dom": "17.0.5",
    "babel-jest": "26.6.3",
    "jest": "26.6.3",
    "react-test-renderer": "17.0.2",
    "testcafe": "1.12.0",
    "typescript": "4.3.2"
  },
  "resolveJs": {
    "isAppTemplate": true
  }
}
