{
  "name": "hello-world",
  "version": "0.30.4",
  "private": true,
  "description": "used as a template for new reSolve applications",
  "scripts": {
    "build": "babel-node ./run.js build",
    "cloud": "babel-node ./run.js cloud",
    "dev": "babel-node ./run.js dev",
    "export-event-store": "babel-node ./run.js export-event-store",
    "import-event-store": "babel-node ./run.js import-event-store",
    "start": "babel-node ./run.js start",
    "test": "jest --testMatch=**/test/unit/*.test.js --verbose --passWithNoTests",
    "test:e2e": "babel-node ./run.js test:e2e"
  },
  "dependencies": {
    "@babel/node": "7.12.13",
    "@resolve-js/eventstore-lite": "0.30.4",
    "@resolve-js/eventstore-postgresql-serverless": "0.30.4",
    "@resolve-js/react-hooks": "0.30.4",
    "@resolve-js/readmodel-lite": "0.30.4",
    "@resolve-js/readmodel-postgresql-serverless": "0.30.4",
    "@resolve-js/scripts": "0.30.4",
    "react": "17.0.1",
    "react-bootstrap": "1.5.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "7.12.13",
    "@babel/plugin-transform-runtime": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "aws-sdk": "2.860.0",
    "aws-xray-sdk-core": "3.2.0",
    "babel-jest": "26.6.3",
    "jest": "26.6.3",
    "react-test-renderer": "17.0.1",
    "testcafe": "1.12.0"
  },
  "resolveJs": {
    "isAppTemplate": true
  }
}
